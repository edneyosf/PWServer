#!/bin/sh

PW_AUTHD=/home/pw/server/authd

cd $PW_AUTHD

java -cp $PW_AUTHD/lib/jio.jar:$PW_AUTHD/lib/application.jar:$PW_AUTHD/lib/commons-collections-3.1.jar:$PW_AUTHD/lib/commons-dbcp-1.2.1.jar:$PW_AUTHD/lib/mysql-connector-java-5.0.8-bin.jar:$PW_AUTHD/lib/commons-pool-1.2.jar:$PW_AUTHD/lib/commons-logging-1.0.4.jar:$PW_AUTHDlib/log4j-1.2.9.jar:.:$CLASSPATH authd table.xml 
sleep 1
